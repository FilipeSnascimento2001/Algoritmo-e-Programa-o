public class Main { //Exemplos 1 de while (contador)
    public static void main(String[] args) {
        int contador;//declarando o contador
        contador = 1; //inicianlizando o contador

        while(contador <=100){
            System.out.print(contador + ",");
            contador++;//mesma coisa que utilizar o contador = contador +1
        }

    }
}


public class Main { //Exemplos de while 2 (declarando números divivel por 2)
    public static void main(String[] args) {
        int contador,resto; //declarando o contador e o resto.
        contador = 0; //inicianlizando o contador

        while(contador <=100) {
            resto = contador %2;

            if(resto==0) {
                System.out.println("O número " + contador + " é par");

            } //imprimir só os numeros divisivel por 2 o codigo para aqui.
            else { //Imprimir todos os números e declarar se é impar ou par, precisa impor a condição else.
                System.out.println("O número " + contador + " é impar");
            } contador++;
        }
    }
}


public class Main { // Exemplo de While 3(acumulador = somar todos os numeros)
    public static void main(String[] args) {
        int contador=1;
        int acumulador=0;

        while(contador <=100) {
            acumulador= acumulador + contador;
            contador++;
        }
        System.out.println("A soma é "+acumulador);
    }
}

import java.util.Scanner;
public class Main { // Exemplo de While 4(media de notas entre 5 alunos)
    public static void main(String[] args) {

        int contador;
        double nota, media,acumuladorNotas;
        contador = 0;
        acumuladorNotas = 0;

        Scanner entrada = new Scanner (System.in);
        while (contador <5) {
            contador++;
            System.out.println("digite uma nota: " + contador);//calcular media
            nota = entrada.nextInt();
            acumuladorNotas = acumuladorNotas + nota;

        }
            media = acumuladorNotas/contador;
            System.out.println("media: "+media);
            entrada.close();
        }
    }






public class Main { //Exercicio 1 Filipe Silva do N. (While)
    public static void main(String[] args) {
        int contador;
        contador = 100;

        while(contador <=200) {
            System.out.print(contador +" ");
            contador++;
        }
    }
}


import java.util.Scanner; //Exercicio 2 Filipe Silva do N. (While)
public class Main {
    public static void main(String[] args) {
        int pares = 0;
        int impares = 0;
        int contador = 0;
        Scanner entrada = new Scanner(System.in);

        while (contador < 10) {
            contador++;
            System.out.println("Digite o " + contador + "º número inteiro.");
            int num = entrada.nextInt();

            if (num % 2 == 0) {
                pares++;
                System.out.println("O número digitado " + num + " é par");
            } else {
                impares++;
                System.out.println("O número digitado " + num + " é impar");

            }

        }
        System.out.println();
        System.out.println("Quantidade de números pares:" + pares);
        System.out.println("Quantidade de números pares:" + impares);
    }
}


import java.util.Scanner; // Exercicio 3 Filipe Silva do N. (While)
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    System.out.println ("Digite o numero intero:");
    int numero = entrada.nextInt();
    int valor = 1;

    while ( valor <= numero  ) {
        System.out.print(valor);
        
        if (valor *2 <= numero) {
        System.out.print(",");
        
       }
        valor *=2;
    }
  }
}


import java.util.Scanner; // Exercicio 4 Filipe Silva do N. (While)
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    double nota;
    
    do { System.out.println("Digite um numero maior ou igual a 10 ou menor ou igual a 20");
    nota = entrada.nextDouble(); 
    }
    
    while (nota <10 || nota >20);
        nota = nota /2;
        System.out.println("o numero dividido por 2 é " + nota);
        entrada.close();
    }
}


public class Main { //Exercicio 5 Filipe Silva do N. (While)
    public static void main(String[] args) {
        int numero = 5;
        int i = 1;      

        System.out.println("Tabuada do " + numero + ":");

        while (i <= 10) {
            System.out.println(numero + " x " + i + " = " + (numero*i));
            i++;
        }
    }
}


import java.util.Scanner; //Exercicio 6 Filipe Silva do N. (While)
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    int numero = 1;
    int resposta;
    int menor = 0; 
    
    boolean primeiroNumero = true;
     
     do { 
     System.out.println("Digite 10 numeros inteiros:");
     resposta = entrada.nextInt();
     
        if (numero < 0) {
           System.out.println ("numero invalido digite novamente.");
     
        }
        else if (primeiroNumero){
            menor = numero;
            primeiroNumero = false;
            numero++;
        }
        else if (numero < menor) {
                menor = numero;
                numero++;
        }
        else {
                    numero++;
        }
        } while (numero <= 10);
                System.out.println("O menor numero digitado é: "+ menor);
                entrada.close();
     }

}

import java.util.Scanner; //Exercicio 7 Filipe Silva do N. (While) 
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    int contador = 1;
    int PessoasSemObesidade = 0;
    
    
    do { 
        System.out.println("Digite qual a sua altura:");
        double altura = entrada.nextDouble();

        System.out.println("Digite qual o seu peso:");
        double peso = entrada.nextDouble();
        
        double imc = peso/ (altura * altura);
        System.out.printf("o IMC da %d: pessoa é: %.2f%n ", contador, imc);
        
        if (imc >= 18.5 && imc <=24.9) {
            PessoasSemObesidade++;
            
        }
        contador++;
    }while (contador <=10);
    System.out.println("quantidade de pessoa com o IMC entre 18,5 e 24,9: "+ PessoasSemObesidade);
    entrada.close();

    }
}



import java.util.Scanner; // Exercicio 8 Filipe Silva do N. (While) 
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    double nota1,nota2,media;
    int aluno =1;
    
    while (aluno <= 5) {
        System.out.println("Aluno " + aluno + ":");
    
    
    do {
        System.out.println("Digite a nota do 1ª nota do Aluno (0 a 10):");
        nota1 = entrada.nextDouble();
        
        
        if(nota1 < 0 || nota1 > 10) {
            System.out.println("Nota invalida, digite novamente a nota da prova:");
          }
        }while (nota1 < 0 || nota1 > 10);
        
    do {
        System.out.println("Digite a nota do 2ª nota do Aluno (0 a 10):");
        nota2 = entrada.nextDouble();
        
        
        if(nota2 < 0 || nota2 > 10) {
            System.out.println("Nota invalida, digite novamente a nota da prova:");
          }
          
        }while (nota2 < 0 || nota2 > 10); 
        
        media = (nota1+nota2)/2;
        System.out.printf("A media do aluno %d é %.2f%n", aluno, media);
        aluno++;
        }
        
        entrada.close();
    }
    
}


import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
    Scanner entrada = new Scanner (System.in);
    int codigo, quantidade;
    double total = 0;
    String produto = "";
    
    int produto = 1;
    
    while (produto <= 2) {
        System.out.println("===Cardapio da lanchonete===")
        System.out.println("Codigo | Produto | Preço")
        System.out.println("100 | Cachorro-quente | R$ 1,20")
        System.out.println("101 | Bauru Simples | R$ 1,30")
        System.out.println("102 | Bauru com ovo | R$ 1,50")
        System.out.println("103 | Hambúrguer | R$ 1,20")
        System.out.println("104 | Cheeseburguer | R$ 1,30")
        System.out.println("105 | Refrigerante | R$ 1,00")
    
    
    do {
        System.out.println("Digite o codigo do " + produto + ":");
        codigo = entrada.nextInt();
        
        System.out.println("Digite a quantidade desejada:");
        quantidade = entrada.nextInt();
        
        
        
        if(codigo < 100 || codigo > 105) {
            System.out.println("Codigo invalida, digite novamente o codigo do produto desejado:");
          }
          
        }while (codigo < 100 || codigo > 105);
        
    do {
        System.out.println("Digite a nota do 2ª nota do Aluno (0 a 10):");
        nota2 = entrada.nextDouble();
        
        
        if(nota2 < 0 || nota2 > 10) {
            System.out.println("Nota invalida, digite novamente a nota da prova:");
          }
          
        }while (nota2 < 0 || nota2 > 10); 
        
        media = (nota1+nota2)/2;
        System.out.printf("A media do aluno %d é %.2f%n", aluno, media);
        aluno++;
        }
        
        entrada.close();
    }
    
}






